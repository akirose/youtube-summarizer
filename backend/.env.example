# Server configuration
PORT=8080

# Google OAuth configuration
GOOGLE_OAUTH_CLIENT_ID=
GOOGLE_OAUTH_CLIENT_SECRET=
GOOGLE_OAUTH_REDIRECT_URI=http://localhost:8080/auth/google/callback

# OpenAI API Key (needed for GPT-4o-mini)
OPENAI_API_KEY=your_openai_api_key_here

# API Key Policy Configuration
# 'all': 모든 사용자가 서버 API 키를 사용 가능
# 'designated': 지정된 사용자만 서버 API 키를 사용 가능
SERVER_OPENAI_API_KEY_POLICY=all
# 지정된 사용자 ID 목록 (쉼표로 구분)
DESIGNATED_USERS=

# Optional configuration
# Enable debug mode
DEBUG=false
